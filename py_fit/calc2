#!/usr/bin/cryoblocks

define THe 0.1765mK
define TSF 0.1384mK

define TCu 0.0743902mK #FIT 0.05:0.2
define HLCu 0.456426nW #FIT 0.1:10
define K1 1e-4 #FIT 0:10000
define K2 0.01nW/K #FIT 0:100
define K3 0.01nW/K #FIT 0:100
define NS1 0.00266392 #FIT 0:1000
define B1 181.761mT #FIT 0:1000

#FIT_COLS 0:1

block bath 300K  type=bath

block  cu ${TCu} type=paramagnet material=copper mass=86.00g
block  he ${THe} type=liquid_he3 volume=17.8cm^3

# Electrons in copper: C=gamma*T, gamma=0.692e-3 per mol
# 1.333679 moles in cell1
block  cue ${THe} type=simple power=1 factor=9.2291e-04

# solid He3 on sinters and aerogels 
# Adsorption: 0.9 cm^3(STP) / m^2 --> 1.089e-3 mole
block   sf ${TSF} type=paramagnet material=he3 moles=${NS1} Bint=${B1}

#link    k1  he  sf type=simple power=1 factor=${K1}
#link   k2  sf  cu type=simple  K=${K2}
#link   k3  sf  cue type=simple  K=${K3}
link   kk  cu cue type=korringa material=copper mass=86.00g
#link   q_cu  bath cu type=const  Qdot=${HLCu}

print_to_file calc2.dat
print_substeps 1
print t T(he) T(sf) T(cue) T(cu) Q(kk)

field 44.8mT

run 30h
